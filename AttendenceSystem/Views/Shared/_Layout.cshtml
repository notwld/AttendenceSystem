<script>
    $.noConflict();
    $(document).ready(function () {
        $('.dhuzz').DataTable()
        $("#delModal").appendTo("body");
    });
    const ConfirmDel = (id) => {
        document.getElementById('delete_course').onclick = () => {
            Delete_Course(id);
        }
    }
    const Delete_Course = (id) => {
        $.ajax({
            url: '/Course/Delete',
            type: 'POST',
            data: { id: id },
            success: function (data) {
                $('#row__' + id).remove();
            },
            error: function (data) {
                alert('Error');
            }
        });
    }
    const CourseEdit = (course_id, course_code, course_name, course_short_name) => {
        document.getElementById("course_id").value = course_id
        document.getElementById("course_code").value = course_code
        document.getElementById("course_name").value = course_name
        document.getElementById("course_short_name").value = course_short_name
    }
    </script>
